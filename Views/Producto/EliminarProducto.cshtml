@model Bodegas.Models.Producto

@{
    ViewData["Title"] = "Eliminar Producto";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Eliminar Producto</h1>



<div class="container">
    <h4>Producto</h4>
    <hr />
    <dl class="row" >
        <dt class="col-sm-2 " hidden ="hidden">
            ID
        </dt>
        <dd class="col-sm-10 active" hidden ="hidden">
            @Html.DisplayFor(model => model.Id_Producto)
        </dd>
        <dt class="col-sm-2 ">
            CODIGO
        </dt>
        <dd class="col-sm-10 ">
            @Html.DisplayFor(model => model.Codigo)
        </dd>
        <dt class="col-sm-2 ">
            PRODUCTO
        </dt>
        <dd class="col-sm-10 ">
            @Html.DisplayFor(model => model.Producto_)
        </dd>
        <dt class="col-sm-2 ">
            ACTIVO
        </dt>
        <dd class="col-sm-10 ">
            @Html.DisplayFor(model => model.Activo)
        </dd>
    </dl>

</div>

@using (Html.BeginForm())
{
    <p>
        <input type="submit" value="Eliminar" class="btn btn-danger" /> 
      
    </p>
}
<div>
    <a asp-action="Index">Back to List</a>
</div>


